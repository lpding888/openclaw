{
  "file": "ui/src/ui/views/skills.ts",
  "description": "Dashboard 技能页面",
  "replacements": {
    "<div class=\"card-title\">Skills</div>": "<div class=\"card-title\">技能</div>",
    "<div class=\"card-sub\">Bundled, managed, and workspace skills.</div>": "<div class=\"card-sub\">内置、托管和工作区技能。</div>",
    "${props.loading ? \"Loading…\" : \"Refresh\"}": "${props.loading ? \"加载中…\" : \"刷新\"}",
    "<span>Filter</span>": "<span>筛选</span>",
    "placeholder=\"Search skills\"": "placeholder=\"搜索技能\"",
    "<div class=\"muted\">${filtered.length} shown</div>": "<div class=\"muted\">显示 ${filtered.length} 项</div>",
    "<div class=\"muted\" style=\"margin-top: 16px\">No skills found.</div>": "<div class=\"muted\" style=\"margin-top: 16px\">未找到技能。</div>",
    "${skill.eligible ? \"eligible\" : \"blocked\"}": "${skill.eligible ? \"可用\" : \"已阻止\"}",
    "<span class=\"chip chip-warn\">disabled</span>": "<span class=\"chip chip-warn\">已禁用</span>",
    "Missing: ${missing.join(\", \")}": "缺少：${missing.join(\", \")}",
    "Reason: ${reasons.join(\", \")}": "原因：${reasons.join(\", \")}",
    "${skill.disabled ? \"Enable\" : \"Disable\"}": "${skill.disabled ? \"启用\" : \"禁用\"}",
    "${busy ? \"Installing…\" : skill.install[0].label}": "${busy ? \"安装中…\" : skill.install[0].label}",
    "reasons.push(\"disabled\");": "reasons.push(\"已禁用\");",
    "reasons.push(\"blocked by allowlist\");": "reasons.push(\"被白名单阻止\");",
    "<span>API key</span>": "<span>API 密钥</span>",
    "Save key": "保存密钥"
  }
}
